#include <bits/stdc++.h>
using namespace std;

int main(){
    int n;
    int m;
    cin>>n>>m;
    int arr[n][m];
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
                cin>>arr[i][j];
        }
    }
    for(int i=0;i<n;i++){
        int j=0;
        int k=m-1;
        while(j<k){
            swap(arr[i][j],arr[i][k]);
            j++;
            k--;
        }
    }for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
    
}
