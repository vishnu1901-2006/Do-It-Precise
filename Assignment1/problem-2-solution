#include <bits/stdc++.h>
using namespace std;

int main(){
    int n;
    int m;
    cin>>n>>m;
    int total=0;
    int indx=0;
    int arr[n][n];
    for(int i=0;i<n;i++){
        int teamtotal=0;
        for(int j=0;j<m;j++){
                cin>>arr[i][j];
                teamtotal+=arr[i][j];
            }if(teamtotal>total){
                total=teamtotal;
                indx=i;
            }
        }cout<<"the team with max score is on index:-"<<indx<<endl;
    }
